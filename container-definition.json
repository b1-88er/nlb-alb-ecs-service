[
  {
    "name": "${container_name}",
    "image": "${image}:${version}",
    "cpu": ${cpu},
    "memoryReservation": ${memory},
    "essential": true,
    "portMappings": [{
        "containerPort": ${container_port},
        "protocol": "tcp"
    }],
    "environment" : [
        { "name" : "API_ENDPOINT", "value" : "${api_endpoint}" }
    ]
  }]}
]
